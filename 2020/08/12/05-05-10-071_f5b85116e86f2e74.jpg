    /** Desktop **/

    @media (max-width: 900px) {
      .col-right {
        display: none;
      }
      .col-main {
        margin-right: 0;
        min-width: initial;
      }
      .cols-wrapper {
        min-width: initial;
      }
      .adb-hero-inner {
        background-image: none;
      }
    }


    /** ADB header breakpoint **/

    @media (max-width: 768px) {
      .adb-header-links {
        display: none;
      }

      .dropdown-nav {
        display: none;
      }

      .adb-header {
        padding: 12px;
        text-align: center;
      }
    }

    @media (max-width: 1196px) {
      .dropdown-nav {
        display: block;
      }

      .breakpoint-hide {
        display:none;
      }

      .more-dropdown {
        height: 50px;
        display: flex;
        align-items: center;
      }
    }

    @media (max-width: 915px) {
      .adb-header-tags {
        display:none;
      }

