      .adb-footer-social {
        float: none;
        margin-bottom: 35px
      }
      .adb-footer-social .adb-footer-social-youtube {
        margin-left: 0;
      }
    }


    /** Small tablet **/

    @media (max-width: 600px) {
      .header-inner .google-logo {
        top: 32px;
        margin-top: 0;
      }
      .header-inner .google-logo img {
        height: 56px;
        width: auto;
